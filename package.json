{
  "name": "saas-template",
  "version": "1.0.0",
  "description": "Complete SaaS application template with Next.js and multiple backend options (Xano, Supabase, InstantDB)",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check .",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up",
    "docker:prod": "docker-compose up",
    "stripe:listen": "stripe listen --forward-to localhost:3000/api/webhooks/stripe",
    "prepare": "husky install",
    "analyze": "cross-env ANALYZE=true npm run build",
    "lighthouse": "lighthouse http://localhost:3000 --output=json --output=html --output-path=./reports/lighthouse",
    "perf:audit": "npm run lighthouse && npm run analyze",
    "bundle:analyze": "npx @next/bundle-analyzer .next/static/chunks/",
    "speed:test": "web-vitals-cli http://localhost:3000",
    "load:test": "artillery run loadtest.yml"
  },
  "dependencies": {
    "next": "^15.0.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "typescript": "^5.7.2",
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "next-auth": "^4.24.10",
    "@next-auth/prisma-adapter": "^1.0.7",
    "tailwindcss": "^3.4.17",
    "postcss": "^8.4.49",
    "autoprefixer": "^10.4.20",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "stripe": "^17.3.1",
    "mapbox-gl": "^3.8.0",
    "react-map-gl": "^7.1.7",
    "@googlemaps/js-api-loader": "^1.16.8",
    "react-google-maps-api": "^2.20.3",
    "axios": "^1.7.9",
    "swr": "^2.2.5",
    "react-query": "^3.39.3",
    "@tanstack/react-query": "^5.62.8",
    "zod": "^3.24.1",
    "react-hook-form": "^7.54.2",
    "@hookform/resolvers": "^3.10.0",
    "react-hot-toast": "^2.4.1",
    "framer-motion": "^11.15.0",
    "lucide-react": "^0.468.0",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-tabs": "^1.1.1",
    "@radix-ui/react-tooltip": "^1.1.4",
    "@radix-ui/react-select": "^2.1.2",
    "@headlessui/react": "^2.2.0",
    "clsx": "^2.1.1",
    "class-variance-authority": "^0.7.1",
    "tailwind-merge": "^2.5.5",
    "@next/bundle-analyzer": "^15.0.3",
    "sharp": "^0.33.5",
    "web-vitals": "^4.2.4",
    "@clerk/nextjs": "^6.9.2",
    "@supabase/supabase-js": "^2.39.8",
    "@instantdb/react": "^0.12.31",
    "pg": "^8.13.1",
    "@prisma/client": "^5.22.0",
    "@upstash/redis": "^1.28.2",
    "jose": "^5.9.6"
  },
  "devDependencies": {
    "@types/mapbox-gl": "^3.4.0",
    "@types/pg": "^8.11.10",
    "vitest": "^2.1.8",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/ui": "^2.1.8",
    "@vitest/coverage-v8": "^2.1.8",
    "@testing-library/react": "^16.1.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/user-event": "^14.5.2",
    "@playwright/test": "^1.49.1",
    "prisma": "^5.22.0",
    "eslint": "^9.17.0",
    "eslint-config-next": "^15.0.3",
    "eslint-plugin-testing-library": "^6.4.0",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "husky": "^9.1.7",
    "lint-staged": "^15.3.0",
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "cross-env": "^7.0.3",
    "lighthouse": "^12.2.1",
    "web-vitals-cli": "^0.3.0",
    "artillery": "^2.0.20",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "saas",
    "nextjs",
    "xano",
    "stripe",
    "mapbox",
    "google-maps",
    "typescript",
    "tailwindcss",
    "template"
  ],
  "author": "Your Name",
  "license": "MIT"
}