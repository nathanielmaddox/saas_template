# =======================
# LOCAL DEVELOPMENT ENV
# =======================
# Copy this to .env.local for local development
# DO NOT commit .env.local to version control

NEXT_PUBLIC_APP_URL=http://localhost:3000
NODE_ENV=development

# Xano Local/Development Instance
NEXT_PUBLIC_XANO_API_URL=https://your-dev-workspace.xano.io/api:your-api-group
XANO_API_KEY=your_dev_xano_api_key

# Test API Keys (use test/sandbox versions)
STRIPE_PUBLISHABLE_KEY=pk_test_
STRIPE_SECRET_KEY=sk_test_
GOOGLE_MAPS_API_KEY=your_test_google_maps_key
NEXT_PUBLIC_MAPBOX_ACCESS_TOKEN=pk.your_test_mapbox_token

# Local Auth Secret (generate with: openssl rand -base64 32)
NEXTAUTH_SECRET=your_local_generated_secret_here
JWT_SECRET=your_local_jwt_secret_here